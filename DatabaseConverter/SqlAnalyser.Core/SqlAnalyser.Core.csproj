<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>victor-wiki</Authors>
    <Description>Parse and generate scripts for TSql, MySql, PlSql and PostgreSql using antlr.</Description>
    <PackageProjectUrl>https://github.com/victor-wiki/DatabaseManager</PackageProjectUrl>
    <Version>2.0.3</Version>
    <Platforms>AnyCPU;x64</Platforms>
    <AssemblyVersion>2.0.3</AssemblyVersion>
    <FileVersion>2.0.3</FileVersion>
    <PackageReleaseNotes>1.postgres trigger: change "execute function" to "execute procedure"
2.fix bug: handle null if postgres procedure no parameter </PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Model\ColumnInfo.cs" />
    <Compile Remove="Model\TableNameInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Antlr4.Runtime.Standard" Version="4.10.1" />
    <PackageReference Include="SqlParserSharp" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\DatabaseInterpreter\DatabaseInterpreter.Core\DatabaseInterpreter.Core.csproj" />
    <ProjectReference Include="..\..\DatabaseInterpreter\DatabaseInterpreter.Model\DatabaseInterpreter.Model.csproj" />
  </ItemGroup>

</Project>
